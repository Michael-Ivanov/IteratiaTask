type Currency {
    id: ID!,
    date: String,
    numCode: Int,
    charCode: String,
    nominal: Int,
    name: String,
    value: Float
}

type Operation {
    id: ID!
    date: String,
    charCode1: String,
    charCode2: String,
    exchangeRate: Float,
    sumToExchange: Float,
    resultSum: Float
}

type Query {
    currencies: [Currency]
    currency(id: ID): Currency

    operations: [Operation]
    operation(id: ID): Operation
}